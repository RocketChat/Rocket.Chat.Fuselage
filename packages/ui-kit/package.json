{
  "name": "@rocket.chat/ui-kit",
  "version": "0.24.0",
  "description": "Interactive UI elements for Rocket.Chat Apps",
  "homepage": "https://rocketchat.github.io/Rocket.Chat.Fuselage/",
  "author": {
    "name": "Rocket.Chat",
    "url": "https://rocket.chat/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RocketChat/Rocket.Chat.Fuselage.git"
  },
  "bugs": {
    "url": "https://github.com/RocketChat/Rocket.Chat.Fuselage/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s .:build:clean .:build:prod",
    ".:build:clean": "rimraf dist",
    ".:build:prod": "webpack --mode production",
    "lint": "eslint --ext js,ts src",
    "lint-fix": "eslint --fix --ext js,ts src",
    "lint-staged": "lint-staged",
    "test": "jest --runInBand",
    "prettier-format": "prettier 'src/**/*.ts' --write",
    "docs": "documentation readme 'src/{,**/!(__mocks__)/**/}!(*.spec).ts' --parse-extension=ts --resolve=node --section='API Reference' --readme-file README.md"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@rocket.chat/eslint-config-alt": "^0.24.0",
    "@rocket.chat/prettier-config": "^0.24.0",
    "@types/jest": "^26.0.23",
    "babel-loader": "^8.2.2",
    "documentation": "^13.2.1",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.1.3",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  }
}
