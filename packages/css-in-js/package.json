{
  "name": "@rocket.chat/css-in-js",
  "version": "0.23.0",
  "description": "Toolset to transpile and use CSS on runtime",
  "homepage": "https://rocketchat.github.io/Rocket.Chat.Fuselage/",
  "author": {
    "name": "Rocket.Chat",
